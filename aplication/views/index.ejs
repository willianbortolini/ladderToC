          <td class="dropzone"><div class="removeBlock" onclick="removeBlock(this)">X</div></td>
          <td class="dropzone"><div class="removeBlock" onclick="removeBlock(this)">X</div></td>
          <td class="dropzone"><div class="removeBlock" onclick="removeBlock(this)">X</div></td>
          <td class="dropzone"><div class="removeBlock" onclick="removeBlock(this)">X</div></td>
