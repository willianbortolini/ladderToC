          </td>
          <td>
            <div class="block" ladderType='lu' draggable="true"><img src="/images/blocks/lineUp.png"></div>
          </td>
        </tr>
        <tr>
          <td>
            <div class="block" ladderType='c' draggable="true"><img src="/images/blocks/coil.png">
              <select name="select-simples-placeholder" class="ls-select">
                <% for(var i=0; i<outputs.length; i++) {%>
                  <option value="<%= outputs[i].port %>">
