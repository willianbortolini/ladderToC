</head>

<body>
  <h3 id="titulo">Novo projeto</h3>
  <div class="project">
    <div class="blocks">
      <table class="tableBlocks">
        <tr>
          <td>
            <div class="block" ladderType='o' draggable="true"><img src="/images/blocks/open.png">
              <select name="select-simples-placeholder" class="ls-select">
                <% for(var i=0; i<inputs.length; i++) {%>
                  <option value="<%= inputs[i].port %>">
                    <%= inputs[i].name %>
                  </option>
                  <% } %>
                    <% for(var i=0; i<outputs.length; i++) {%>
                      <option value="<%= outputs[i].port %>">
                        <%= outputs[i].name %>
                      </option>
                      <% } %>
              </select>
            </div>
          </td>
