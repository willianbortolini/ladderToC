            </div>
          </td>
        </tr>
        <tr>
          <td>
            <div class="block" ladderType='pd' draggable="true"><img src="/images/blocks/pulseDown.png">
              <select name="select-simples-placeholder" class="ls-select">
                <% for(var i=0; i<inputs.length; i++) {%>
                  <option value="<%= inputs[i].port %>">
                    <%= inputs[i].name %>
